{
    "name": "bear/sunday",
    "type": "library",
    "description": "BEAR.Sunday, a resource oriented framework",
    "version": "0.4.2-dev",
    "keywords": ["rest", "framework", "api"],
    "homepage": "https://github.com/koriym/BEAR.Sunday",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com",
            "homepage": "https://github.com/koriym"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "ext-apc": "*",
        "ext-curl": "*",
        "aura/autoload": "dev-master",
        "aura/di": "dev-master",
        "aura/router": "dev-master",
        "aura/signal": "dev-master",
        "aura/web": "dev-master",
        "bear/resource": "dev-master",
        "doctrine/common": "2.3.*-dev",
        "doctrine/dbal": "2.3.*-dev",
        "facebook/xhprof": "0.9.*",
        "guzzle/guzzle": "2.7.*",
        "pagerfanta/pagerfanta": "dev-master",
        "smarty/smarty": "3.1.*",
        "symfony/console": "2.0.*",
        "symfony/http-foundation": "2.0.*",
        "ray/aop": "dev-master",
        "ray/di": "dev-master",
        "vdump/vdump": "*",
        "printo/printo": "*",
        "firephp/firephp-core": "dev-master",
        "twitter/bootstrap": "dev-master",
        "nocarrier/hal": "dev-master",
        "zendframework/zend-log": "2.0.*"
    },
    "autoload": {
        "psr-0": {
            "BEAR\\Framework": "src/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "crodas/Haanga": "dev-master",
        "twig/twig": "dev-master",
        "zend/zend-log1": "1.11",
        "zend/zend-cache1": "1.11",
        "zendframework/zend-loader": "2.0.*-dev",
        "zendframework/zend-eventmanager": "2.0.*-dev",
        "zendframework/zend-servicemanager": "2.0.*-dev",
        "zendframework/zend-cache": "2.0.*-dev",
        "zendframework/zend-log": "2.0.*-dev",
        "zendframework/zend-stdlib": "2.0.*-dev",
        "crodas/haanga": "dev-master",
        "twig/twig": "dev-master",
        "monolog/monolog": "1.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "crodas/haanga",
                "version": "1.0.4",
                "dist": {
                    "url": "https://github.com/crodas/Haanga/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/crodas/Haanga.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "vdump/vdump",
                "version": "0.1.0",
                "target-dir": "vdump",
                "dist": {
                    "url": "https://github.com/koriym/vdump/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/koriym/vdump.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "facebook/xhprof",
                "version": "0.9.2",
                "dist": {
                    "url": "https://github.com/facebook/xhprof/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/facebook/xhprof.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
           "type": "package",
           "package": {
               "version": "master",
               "name": "twitter/bootstrap",
               "source": {
                   "url": "https://github.com/twitter/bootstrap.git",
                   "type": "git",
                   "reference": "master"
               },
               "dist": {
                   "url": "https://github.com/twitter/bootstrap/zipball/master",
                   "type": "zip"
               }
           }
       },
       {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
       }
    ]
}
